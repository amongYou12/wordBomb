--------------------------------------------------------------------------------
-- 4. UTILITY FUNCTIONS
--------------------------------------------------------------------------------
local function IDA_tableItemExists(array, val)
	if not array then return false end
	for i, v in pairs(array) do
		if v == val then return true end
	end
	return false
end

local function IDA_sanitizeInput(str)
	if not str then return "" end
	local s = tostring(str)
	-- trim
	local starti = 1
	local endi = #s
	while starti <= #s and s:sub(starti, starti):match("%s") do
		starti = starti + 1
	end
	while endi > 0 and s:sub(endi, endi):match("%s") do
		endi = endi - 1
	end
	if starti > endi then return "" end
	return s:sub(starti, endi)
end

--------------------------------------------------------------------------------
-- 5. MAIN LOGIC SCRIPT
--------------------------------------------------------------------------------
local function IDA_MainScript()
	local script = Instance.new("LocalScript", MainFrame)

	-- Robust Global Environment
	local function GetGlobalEnv()
		if getgenv then return getgenv() end
		if shared then return shared end
		return _G
	end
	
	local GlobalEnv = GetGlobalEnv()
	local HISTORY_KEY = "_UNPATCHABOMB_HISTORY_V4"

	if not GlobalEnv[HISTORY_KEY] then
		GlobalEnv[HISTORY_KEY] = {}
	end
	
	local IDA_alreadyUsed = GlobalEnv[HISTORY_KEY]
	local IDA_LastInput = ""

	local IDA_LONG_WORD_MIN = 10 
	local IDA_WORD_DELAY = 1.2 

	-- Find word with Anti-Freeze support (Dynamic Tables)
	local function IDA_findWord(substr)
		if not substr then return nil end
		local s = string.lower(substr)
		if s == "" then return nil end
		
		-- Iterate through ALL tables in ALL_WORD_LISTS
		for listIndex, wordTable in ipairs(ALL_WORD_LISTS) do
    		if wordTable and #wordTable > 0 then
    			for i, v in ipairs(wordTable) do
    				-- Anti-Freeze: Yield every 500 checks
    				if i % 500 == 0 then task.wait() end
    				
    				if not IDA_tableItemExists(IDA_alreadyUsed, v) then
    					if #v >= IDA_LONG_WORD_MIN then
    						if string.find(string.lower(v), s, 1, true) then
    							table.insert(IDA_alreadyUsed, v)
    							return v
    						end
    					end
    				end
    			end
    		end
		end
		
		return nil
	end

	-- REALISTIC AUTO TYPE (FASTER + RAMP UP)
	local function IDA_autoType(word)
		if not word or word == "" then return end

		local success, vim = pcall(function() return game:GetService("VirtualInputManager") end)
		
		local function pressKey(keyEnum)
			if not keyEnum then return end
			pcall(function()
				vim:SendKeyEvent(true, keyEnum, false, game)
				task.wait() 
				vim:SendKeyEvent(false, keyEnum, false, game)
			end)
		end

		if success and vim then
			local i = 1
			local mistakeTriggered = false
			local chanceOfMistake = 0.15 

			while i <= #word do
				-- Mistake Logic (>10 chars only)
				if #word > 10 and not mistakeTriggered and i > 4 and i < (#word - 2) and math.random() < chanceOfMistake then
					mistakeTriggered = true
					
					-- Thinking pause
					task.wait(math.random(100, 250) / 1000)

					local deleteCount = math.random(2, 3)
					if deleteCount >= i then deleteCount = i - 1 end 

					for b = 1, deleteCount do
						pressKey(Enum.KeyCode.Backspace)
						task.wait(math.random(50, 100) / 1000)
					end

					i = i - deleteCount
					if i < 1 then i = 1 end

					task.wait(math.random(100, 250) / 1000)
				else
					-- Normal Type
					local ch = word:sub(i, i)
					local up = string.upper(ch)
					
					if up:match("%u") and #up == 1 and Enum.KeyCode[up] then
						pressKey(Enum.KeyCode[up])
					elseif ch:match("%d") then
						local map = {["0"]="Zero",["1"]="One",["2"]="Two",["3"]="Three",["4"]="Four",["5"]="Five",["6"]="Six",["7"]="Seven",["8"]="Eight",["9"]="Nine"}
						local name = map[ch]
						if name and Enum.KeyCode[name] then
							pressKey(Enum.KeyCode[name])
						end
					elseif ch == " " then
						pressKey(Enum.KeyCode.Space)
					else
						pcall(function() setclipboard(ch) end) 
					end
					
					i = i + 1
				end

				-- Speed Calculation
				local delay = 0
				if #word >= 13 and i <= 3 then
					-- Start Speed
					delay = math.random(100, 160) / 1000
				else
					-- Normal/Fast Speed
					delay = math.random(20, 70) / 1000
				end
				
				task.wait(delay)
			end

			-- Enter
			task.wait(math.random(80, 150) / 1000)
			pcall(function()
				vim:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
				vim:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
			end)
		else
			-- Fallback
			pcall(function() setclipboard(word) end)
			task.wait(0.04)
			local ok, vm = pcall(function() return game:GetService("VirtualInputManager") end)
			if ok and vm then
				pcall(function()
					vm:SendKeyEvent(true, Enum.KeyCode.Return, false, game)
					vm:SendKeyEvent(false, Enum.KeyCode.Return, false, game)
				end)
			end
		end

		task.wait(IDA_WORD_DELAY)
	end
	
	-- Search Handler
	local function IDA_PerformSearch(inputText)
		local sanitized = IDA_sanitizeInput(inputText)
		
		-- Empty check
		local hasAnyWords = false
		for _, list in ipairs(ALL_WORD_LISTS) do
		    if #list > 0 then hasAnyWords = true break end
		end
		
		if not hasAnyWords then
			script.Parent.WordText.Text = "Tables are empty! Paste words in script."
			return
		end
		
		IDA_LastInput = sanitized
		
		if sanitized == "" then
			script.Parent.WordText.Text = "Please type something first."
			return
		end

		script.Parent.WordText.Text = sanitized .. " - Searching..."
		task.wait(0.05) 

		local success, result = pcall(function()
			local found = IDA_findWord(sanitized)
			if found then
				return found
			else
				-- Relaxed Search (Check all tables again)
				local fallback = nil
        		for _, wordTable in ipairs(ALL_WORD_LISTS) do
            		if wordTable and #wordTable > 0 then
            			for i, v in ipairs(wordTable) do
            				if i % 500 == 0 then task.wait() end
            				if not IDA_tableItemExists(IDA_alreadyUsed, v) then
            					if string.find(string.lower(v), string.lower(sanitized), 1, true) then
            						table.insert(IDA_alreadyUsed, v)
            						return v
            					end
            				end
            			end
            		end
        		end
				return fallback, true
			end
		end)

		if success then
			local word = result
			-- Small Lua trick for multiple returns handling:
			-- Since pcall returns (true, val1, val2), 'result' is val1.
			-- We don't strictly need val2 (isFallback) to make it work, 
			-- but we can infer if the word matches the logic.
			
			if word and type(word) == "string" then
				script.Parent.WordText.Text = "'" .. word .. "' found."
				pcall(function() setclipboard(word) end)
				task.wait(0.18)
				IDA_autoType(word)
			else
				script.Parent.WordText.Text = "No unused words found."
			end
		else
			script.Parent.WordText.Text = "Search Error."
			warn(result)
		end
	end

	-- Handlers
	local function IDA_onFocusLost()
		IDA_PerformSearch(script.Parent.LetterBox.Text)
	end
	
	local function IDA_onRerunClick()
		if IDA_LastInput == "" then
			IDA_PerformSearch(script.Parent.LetterBox.Text)
		else
			IDA_PerformSearch(IDA_LastInput)
		end
	end
	
	local function IDA_onResetClick()
	    for k in pairs(IDA_alreadyUsed) do
	        IDA_alreadyUsed[k] = nil
	    end
	    script.Parent.WordText.Text = "History Reset!"
	end

	script.Parent.LetterBox.FocusLost:Connect(IDA_onFocusLost)
	script.Parent.RerunButton.MouseButton1Click:Connect(IDA_onRerunClick)
	script.Parent.ResetButton.MouseButton1Click:Connect(IDA_onResetClick)
end

coroutine.wrap(IDA_MainScript)()

-- Animations & Dragging
local function AnimScript() 
	local script = Instance.new('LocalScript', UnpatchabombFrame)
	wait(1) 
	script.Parent.IntroFrame:TweenPosition(UDim2.new(0,0,1,0), "Out", "Quad", 0.5, true)
	local Open1 = false
	script.Parent.TopFrame.MinimizeButton.MouseButton1Click:connect(function()
		if Open1 == false then
			Open1 = true
			script.Parent.MainFrame:TweenSize(UDim2.new(1,0,0,0), "Out", "Quad", 0.5, true)
			script.Parent.IntroFrame:TweenSize(UDim2.new(1,0,0,0), "Out", "Quad", 0.5, true)
		else
			Open1 = false
			script.Parent.MainFrame:TweenSize(UDim2.new(1,0,1,0), "Out", "Quad", 0.5, true)
			script.Parent.IntroFrame:TweenSize(UDim2.new(1,0,1,0), "Out", "Quad", 0.5, true)
		end
	end)
end
coroutine.wrap(AnimScript)()

local function DragScript()
	local script = Instance.new('LocalScript', UnpatchabombFrame)
	local UserInputService = game:GetService("UserInputService")
	local gui = script.Parent
	local dragging, dragInput, dragStart, startPos
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
	end
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then dragging = false end
			end)
		end
	end)
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then update(input) end
	end)
end
coroutine.wrap(DragScript)()

local function LoadAnim()
	local script = Instance.new('LocalScript', Text3)
	while true do
		script.Parent.Text = "Loading."
		wait(0.1)
		script.Parent.Text = "Loading.."
		wait(0.1)
		script.Parent.Text = "Loading..."
		wait(0.1)
	end
end
coroutine.wrap(LoadAnim)()
