--[[
    UNPATCHABOMB [DYNAMIC TABLE SUPPORT]
    - NOW SUPPORTS UNLIMITED WORD TABLES.
    - Just add { "word", "word" }, inside the ALL_WORD_LISTS table.
    - Features: Realistic Typing, Rerun, Reset, Anti-Freeze, Global Persistence.
]]--

--------------------------------------------------------------------------------
-- 1. ROBUST GUI CLEANUP & PARENTING
--------------------------------------------------------------------------------
local CoreGui = game:GetService("CoreGui")
local Players = game:GetService("Players")
local ParentTarget = nil

pcall(function()
    if CoreGui:FindFirstChild("Unpatchabomb") then
        CoreGui.Unpatchabomb:Destroy()
    end
    ParentTarget = CoreGui
end)

if not ParentTarget then
    local Player = Players.LocalPlayer
    local PlayerGui = Player:WaitForChild("PlayerGui", 5)
    if PlayerGui then
        if PlayerGui:FindFirstChild("Unpatchabomb") then
            PlayerGui.Unpatchabomb:Destroy()
        end
        ParentTarget = PlayerGui
    end
end

if not ParentTarget then
    warn("Unpatchabomb Error: Could not find a valid GUI parent.")
    return
end

